@import url("https://fonts.googleapis.com/css?family=Ubuntu&display=swap");body{color:#769fcd}#container{font-family:"Ubuntu", sans-serif;min-height:100vh;display:flex;flex-direction:column;background:#f7fbfc;cursor:default}#container ::selection{background-color:#473ea2;color:#dbd8f8}header{background-color:#769fcd;color:#f7fbfc;height:80px}header a{color:#f7fbfc}header ul{display:flex;flex-wrap:wrap;justify-content:space-between;align-items:center}header ul li a{color:#f7fbfc;text-decoration:none}header ul li a:hover{color:#f7fbfc}header ul li.logo{flex:1;height:80px;background:url("/images/juggy_head.png") no-repeat left bottom}header ul li.toggle{order:1;font-size:2em;padding-right:0.5em}header ul li.menu-item{width:100%;text-align:center;padding:0.3em 0;order:3;background-color:#769fcd;display:none;font-size:1.5em}header ul li.menu-item.active{display:block;z-index:10}header ul li.menu-item.active:nth-child(2){padding-top:1.5em}header ul li.menu-item a{width:100%;display:block}main{flex-grow:1;display:flex;flex-direction:column}main a{color:#635ab9;text-decoration:none}main a:hover{color:#de2217}aside.left{order:2;display:none}aside.right{order:3}#content{margin:1em;flex-grow:1;order:2}footer{padding:0 1em 0.5em 1em;background:linear-gradient(rgba(247,251,252,0.5), rgba(185,215,234,0.8))}footer a{color:#635ab9;text-decoration:none}footer a:hover{color:#de2217}footer p{margin:0.3em 0}@media all and (min-width: 1200px){aside.left{order:1;display:block}}@media all and (min-width: 950px){#container{background:#f7fbfc url("/images/toursJuggy_t08.png") no-repeat bottom 50px right 75px}main{flex-direction:row}aside{width:200px}aside.right{order:3}header ul li.menu-item{display:block;width:auto;padding-bottom:0.8em;align-self:flex-end;font-size:1em;padding:0 1em 1em 0;white-space:nowrap}header ul li.toggle{display:none}}
